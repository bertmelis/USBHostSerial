    },
    {
      "name": "Silicon Labs CP210x USB-UART converter driver",
      "version": "https://components.espressif.com/api/download/?object_type=component&object_id=4d0a4060-9c0d-41bc-89cf-729f93ec82ed"
    },
    {
      "name": "FTDI UART-USB converters driver",
      "version": "https://components.espressif.com/api/download/?object_type=component&object_id=57dcefc5-645d-4db4-9d3c-b78cb8f77fc0"
    }
  ],
  "build":
  {
    "libLDFMode": "deep+"
  }
}
