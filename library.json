{
  "name": "USBHostSerial",
  "keywords": "Serial, USB, host, esp32-s3",
  "description": "Use ESP32-S3 USB host as Serial",
  "authors":
  {
    "name": "Bert Melis",
    "url": "https://github.com/bertmelis"
  },
  "license": "MIT",
  "homepage": "https://github.com/bertmelis/USBHostSerial",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/bertmelis/USBHostSerial.git"
  },
  "version": "0.0.1",
  "frameworks": "arduino",
  "platforms": ["espressif32"],
  "headers": ["USBHostSerial.h"],
  "dependencies":
