    {
      "name": "Espressif's additions to TinyUSB",
      "version": "https://components.espressif.com/api/download/?object_type=component&object_id=65318090-af6e-4dbe-a257-5074ed07a337"
    },
    {
      "name": "Espressif TinyUSB fork",
      "version": "https://components.espressif.com/api/download/?object_type=component&object_id=55142eec-a3a4-47a5-ad01-4ba3ef44444b"
    },
    {
      "name": "Virtual COM Port Service",
      "version": "https://components.espressif.com/api/download/?object_type=component&object_id=569aae85-0f1c-4cf9-ac7f-0f2f28024951"
    },
    {
      "name": "USB Host CDC-ACM Class Driver",
      "version": "https://components.espressif.com/api/download/?object_type=component&object_id=4c8aa513-4d20-4c79-8089-a5205746ef48"
